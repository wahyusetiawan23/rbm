    <script>
      function myFunction() {
        var checkBox = document.getElementById("myCheck");
        var form = document.getElementById("formInput");
        if (checkBox.checked == false) {
          form.style.visibility = "";
        } else {
          form.style.visibility = "hidden";
        }
      }
    </script>
